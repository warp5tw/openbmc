From 6a931cd8b130f543c7e276fd340655ce94487d0b Mon Sep 17 00:00:00 2001
From: kfting <kfting@nuvoton.com>
Date: Tue, 30 Jun 2020 13:39:10 +0800
Subject: [PATCH] Modify Olympus flash size from 32MB to 64MB

Signed-off-by: kfting <kfting@nuvoton.com>
---
 arch/arm/boot/dts/nuvoton-npcm750-runbmc-olympus.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/nuvoton-npcm750-runbmc-olympus.dts b/arch/arm/boot/dts/nuvoton-npcm750-runbmc-olympus.dts
index a55dccc51ff4..9d137bba600e 100644
--- a/arch/arm/boot/dts/nuvoton-npcm750-runbmc-olympus.dts
+++ b/arch/arm/boot/dts/nuvoton-npcm750-runbmc-olympus.dts
@@ -186,11 +186,11 @@
 					};
 					rofs@800000 {
 						label = "rofs";
-						reg = <0x800000 0x1500000>;
+						reg = <0x800000 0x3500000>;
 					};
 					rwfs@1d00000 {
 						label = "rwfs";
-						reg = <0x1d00000 0x300000>;
+						reg = <0x3d00000 0x300000>;
 					};
 				};
 			};
-- 
